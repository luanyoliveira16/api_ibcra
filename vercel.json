{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/routes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "ignoreCommand": "git check-ignore -v $(git ls-files)"
}
